{
  "name": "cloudflare-pages-function-next-js",
  "packageManager": "npm@9.6.6",
  "type": "module",
  "dependencies": {
    "next": "^13.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^0.10.1",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.3",
    "better-sqlite3": "^8.3.0",
    "drizzle-kit": "^0.17.6",
    "drizzle-orm": "^0.25.4",
    "eslint-config-next": "^13.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4",
    "vercel": "^28.20.0",
    "wrangler": "^2.19.0"
  },
  "scripts": {
    "build": "next build",
    "clean": "rimraf .next .vercel",
    "dev:next": "npx @cloudflare/next-on-pages --watch",
    "dev:wrangler": "NODE_ENV=development wrangler pages dev ./.vercel/output/static --compatibility-flag=nodejs_compat --compatibility-date=2023-05-04 --d1 DB --ip ::0 --ip 0.0.0.0 --live-reload --persist",
    "dev": "npx @cloudflare/next-on-pages && run-p -s \"dev:*\"",
    "type-check": "tsc --noEmit"
  }
}
